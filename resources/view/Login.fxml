<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/25"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.gudang.controller.LoginController"
      prefHeight="200.0" prefWidth="300.0">
   <children>
      <Label text="Login Aplikasi Gudang" />
      <TextField fx:id="txtuser" promptText="Username" />
      <PasswordField fx:id="txtpass" promptText="Password" />
      <Button fx:id="btnLogin" onAction="#loginAction" text="Login" />
      <Label fx:id="lblStatus" text="" />
   </children>
</VBox>